		"serve-favicon": "2.5.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@types/body-parser": "1.19.5",
		"@types/cookie-parser": "1.4.7",
		"@types/debug": "4.1.12",
		"@types/marked": "5.0.2",
		"@types/morgan": "1.9.9",
		"@types/node": "20.12.10",
		"@types/power-assert": "1.5.12",
		"@types/serve-favicon": "2.5.7",
		"browser-sync": "3.0.2",
